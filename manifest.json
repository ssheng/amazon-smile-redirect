{
  "manifest_version": 2,
  "name": "Amazon Smile Auto Redirect",
  "version": "1.0",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png"
  },
  "background": {
    "scripts": ["main.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.amazon.com/"]
}